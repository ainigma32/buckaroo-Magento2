{
    "name": "buckaroo/magento2",
    "description": "Buckaroo Magento 2 extension",
    "require": {
        "php": "^8.0 || ^8.1 || ^8.2 || ^8.3 || ^8.4",
        "magento/module-payment": ">=100.3.3",
        "magento/framework": ">=103.0.0",
        "buckaroo/sdk": "^1.19.0",
        "monolog/monolog": "^2.9 || ^3.0",
        "phpunit/phpunit": "^9.5 || ^10.5",
        "psr/log": "^2.0 || ^3.0",
        "ext-json": "*",
        "ext-pcre": "*"
    },
    "type": "magento2-module",
    "license": "CC-BY-NC-ND-3.0",
    "authors": [
        {
            "name": "Buckaroo",
            "email": "support@buckaroo.nl",
            "homepage": "https://www.buckaroo.nl"
        }
    ],
    "support": {
        "email": "support@buckaroo.nl",
        "issues": "https://support.buckaroo.nl/contact",
        "wiki": "https://support.buckaroo.nl",
        "docs": "https://docs.buckaroo.io/"
    },
    "homepage": "https://www.buckaroo.nl",
    "version" : "v2.0.0-RC2",
    "minimum-stability": "RC",
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Buckaroo\\Magento2\\": ""
        }
    },
    "require-dev": {
        "php-webdriver/webdriver": "^1.1",
        "squizlabs/php_codesniffer": "^3.7",
        "phpmd/phpmd": "^2.13",
        "phpstan/phpstan": "^1.10",
        "magento/magento-coding-standard": "^37"
    },
    "repositories": {
        "magento": {
            "type": "composer",
            "url": "https://repo.magento.com/"
        }
    },
    "config": {
        "allow-plugins": {
            "magento/composer-dependency-version-audit-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "replace": {
        "buckaroo/magento2secondchance": "*"
    },
    "suggest": {
        "buckaroo/magento2secondchance": "No longer needed â€“ remove it"
    }
}
